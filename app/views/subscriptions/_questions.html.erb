<%= form_with model: survey, data: { controller: 'nested-form', nested_form_wrapper_selector_value: '.nested-form-wrapper' } do |f| %>

  <%= f.fields_for :survey_questions do |answer_fields| %>
    <%= render "subscriptions/answer_form", f: answer_fields, f_parent: f %>
  <% end %>

  <div data-nested-form-target="target"></div>
<% end %>